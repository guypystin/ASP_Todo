@model IEnumerable<Mission>


@foreach (var item in Model)
{

    <tr id="@item.Id">
        <td>
            @Html.CheckBoxFor(modelItem => item.Complete, new
            {
                id = item.Id,
                @class = "ActiveCheck",
                @onclick = "checkboxClick(this)",
            })
        </td>
        <td class="editable">@item.Name</td>
        <td>@item.Add_Time</td>
        <td>
            <a class="mission__table-edit-a unselectable" onclick="editMission(`@item.Id`, `@item.Name`)">
                <img class="mission__table-edit edit" src="/img/edit.svg" alt="Alternate Text" />
            </a>
        </td>
        <td>
            <a onclick="deleteMission(@item.Id)" class="unselectable">
                <img class="sidebar__delete-img " src="/img/close.png" alt="Alternate Text" />
            </a>
        </td>

    </tr>
}

